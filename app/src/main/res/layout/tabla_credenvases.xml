        android:layout_weight="2"
        android:text="@string/table_sku"
        style="@style/tv_titulos.normal"
        />

    <TextView
        android:padding="@dimen/paddingCardView"
        android:id="@+id/dat3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:text="@string/table_sku"
        style="@style/tv_titulos.normal"
        />

    <TextView
        android:padding="@dimen/paddingCardView"
        android:id="@+id/dat4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:text="@string/table_sku"
        style="@style/tv_titulos.normal"
        />

</TableRow>